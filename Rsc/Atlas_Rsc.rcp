ALERT ID kRomIncompatibleAlert
    DEFAULTBUTTON 0
    ERROR
BEGIN
    TITLE "System Incompatible"
    MESSAGE "System Version 5.0 or greater is required to run this application."
    BUTTONS "OK"
END

ALERT ID kPrivateBrowsingAlert
CONFIRMATION
BEGIN
    TITLE "Private Browsing"
    MESSAGE "Private browsing is currently: ^1\n" \
            "Would you like to enable or disable private browsing?"
    BUTTONS "Enable" "Disable" "Cancel"
END

