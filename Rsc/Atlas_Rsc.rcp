STRING ID 9990 "Web Browser"	// Clie launcher description.

MENU ID kMainMenuBar
BEGIN
    PULLDOWN "Page"
    BEGIN
        MENUITEM "Toggle Private Browsing..." ID kMainMenuPageTogPrivBrows
        MENUITEM "Private Browsing Options..." ID kMainMenuPagePrivBrowsOp
        MENUITEM SEPARATOR
        MENUITEM "History" ID kMainMenuPageHistory "Y"
        MENUITEM "Bookmarks" ID kMainMenuPageBookmarks "V"
        MENUITEM "Add Bookmark..." ID kMainMenuPageAddBkmrk "A"
        MENUITEM SEPARATOR
        MENUITEM "Page Info" ID kMainMenuPageInfo
        MENUITEM "Find on Page..." ID kMainMenuPageFind "F"
        MENUITEM SEPARATOR
        MENUITEM "Send Page..." ID kMainMenuPageSend
        MENUITEM "Send Page Address..." ID kMainMenuPageSendURL
    END

    PULLDOWN "Go"
    BEGIN
        MENUITEM "Back" ID kMainMenuGoBack "B"
        MENUITEM "Forward" ID kMainMenuGoForward "N"
        MENUITEM "Refresh" ID kMainMenuGoRefresh "R"
        MENUITEM "Home" ID kMainMenuGoHome "H"
        MENUITEM SEPARATOR
        MENUITEM "Open Address..." ID kMainMenuGoOpenURL "O"
    END

    PULLDOWN "Edit"
    BEGIN
        MENUITEM "Undo" ID kMainMenuEditUndo "U"
        MENUITEM "Cut" ID kMainMenuEditCut "X"
        MENUITEM "Copy"ID kMainMenuEditCopy "C"
        MENUITEM "Paste" ID kMainMenuEditPaste "P"
        MENUITEM "Select All" ID kMainMenuEditSelectAll "S"
        MENUITEM SEPARATOR
        MENUITEM "Keyboard" ID kMainMenuEditKeyboard "K"
        MENUITEM "Graffiti Help" ID kMainMenuEditGraffitiHelp "G"
    END

    PULLDOWN "Options"
    BEGIN
        MENUITEM " \x95 Desktop View" ID kMainMenuOptionsDesktopView_Active
        MENUITEM "   Desktop View" ID kMainMenuOptionsDesktopView
        MENUITEM " \x95 Mobile View" ID kMainMenuOptionsMobileView_Active
        MENUITEM "   Mobile View" ID kMainMenuOptionsMobileView
        MENUITEM SEPARATOR
        MENUITEM "Preferences..." kMainMenuOptionsPrefs "E"
        MENUITEM "About Atlas" ID kMenuOptionsAboutAtlas
    END

END

